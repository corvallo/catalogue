{
  "name": "@catalogue/tools",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "catalogue": "dist/cli.js",
    "catalog-index": "dist/catalog-index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./plugin-api": {
      "types": "./dist/plugin-api.d.ts",
      "default": "./dist/plugin-api.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "default": "./dist/core.js"
    }
  },
  "files": ["dist", "app"],
  "scripts": {
    "build": "tsc -p tsconfig.json && node scripts/copy-app.mjs",
    "dev": "tsc -p tsconfig.json --watch",
    "lint": "echo \"no lint configured\"",
    "test": "echo \"no tests\""
  },
  "dependencies": {
    "@catalogue/cli": "0.0.0",
    "@catalogue/app": "0.0.0",
    "@catalogue/plugin-api": "0.0.0",
    "@catalogue/indexer-generated": "0.0.0",
    "@catalogue/core": "0.0.0",
    "@catalogue/indexer-vite": "0.0.0",
    "@catalogue/renderer-react": "0.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "chokidar": "^4.0.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.474.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.28.0",
    "vite": "^7.2.4",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
