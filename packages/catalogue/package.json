{
  "name": "@catalogue-lab/catalogue",
  "version": "0.0.5",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/corvallo/catalogue.git"
  },
  "homepage": "https://github.com/corvallo/catalogue",
  "bugs": {
    "url": "https://github.com/corvallo/catalogue/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "catalogue": "dist/cli.js",
    "catalog-index": "dist/catalog-index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./plugin-api": {
      "types": "./dist/plugin-api.d.ts",
      "default": "./dist/plugin-api.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "default": "./dist/core.js"
    }
  },
  "files": [
    "dist",
    "app",
    "node_modules/@catalogue-lab"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "prepack": "node scripts/copy-app.mjs && node scripts/copy-internal.mjs",
    "postpack": "node scripts/clean-app.mjs && node scripts/clean-internal.mjs",
    "lint": "echo \"no lint configured\"",
    "test": "echo \"no tests\""
  },
  "dependencies": {
    "@catalogue-lab/cli": "0.0.0",
    "@catalogue-lab/app": "0.0.0",
    "@catalogue-lab/plugin-api": "0.0.0",
    "@catalogue-lab/indexer-generated": "0.0.0",
    "@catalogue-lab/core": "0.0.0",
    "@catalogue-lab/indexer-vite": "0.0.0",
    "@catalogue-lab/renderer-react": "0.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "fast-glob": "^3.3.2",
    "chokidar": "^4.0.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.474.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.28.0",
    "vite": "^7.2.4",
    "zustand": "^5.0.2"
  },
  "bundledDependencies": [
    "@catalogue-lab/app",
    "@catalogue-lab/cli",
    "@catalogue-lab/core",
    "@catalogue-lab/plugin-api",
    "@catalogue-lab/indexer-generated",
    "@catalogue-lab/indexer-vite",
    "@catalogue-lab/renderer-react"
  ],
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
