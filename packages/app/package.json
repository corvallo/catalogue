{
  "name": "@catalogue-lab/app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "src",
    "index.html",
    "vite.config.ts",
    "tsconfig.json",
    "catalog.config.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.json && vite build",
    "preview": "vite preview",
    "index:generate": "pnpm --filter @catalogue-lab/indexer-generated build && catalog-index --patterns \"${CATALOGUE_STORIES:-src/**/*.stories.tsx}\" --output \"${CATALOGUE_INDEX_OUTPUT:-src/stories-index.ts}\""
  },
  "dependencies": {
    "@catalogue-lab/core": "0.0.0",
    "@catalogue-lab/indexer-generated": "0.0.0",
    "@catalogue-lab/indexer-vite": "0.0.0",
    "@catalogue-lab/plugin-api": "0.0.0",
    "@catalogue-lab/renderer-react": "0.0.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.474.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.28.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  }
}
