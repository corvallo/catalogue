{
  "name": "@catalogue/indexer-generated",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "default": "./dist/node.js"
    }
  },
  "bin": {
    "catalog-index": "dist/cli.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "lint": "echo \"no lint configured\"",
    "test": "echo \"no tests\""
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "dependencies": {
    "@catalogue/core": "0.0.0",
    "@catalogue/plugin-api": "0.0.0",
    "fast-glob": "^3.3.2"
  }
}
